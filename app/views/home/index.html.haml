%script{:src => "https://maps.googleapis.com/maps/api/js?key=AIzaSyBDAmzYeQpRVm_0ajvbf5GVqgislxyTqKc"}

.row
  .col-md-9
    #googleMap{style: 'width:100%;height:400px;'}

    #coordinates
      - @users.each do |user|
        .user-data{ data: {name: user.name }}
          = user.fetch_coordinates
  .col-lg-3
    .well.sidebar-nav
      %h3 Users
      %ul.nav.nav-list
        -@users.each do |user|
          %li= link_to user.name, user_path(user)
